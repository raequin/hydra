  <!-- ROS base_link to UR 'Base' Coordinates transform -->
  <link name="attacher_base"/>
  <joint name="attacher_base_link-base_fixed_joint" type="fixed">
    <!-- NOTE: this rotation is only needed as long as base_link itself is
                 not corrected wrt the real robot (ie: rotated over 180
                 degrees)
      -->
    <origin rpy="0 0 -3.14159265359" xyz="0 0 0"/>
    <parent link="attacher_base_link"/>
    <child link="attacher_base"/>
  </joint>
  <!-- Frame coincident with all-zeros TCP on UR controller -->
  <link name="attacher_tool0"/>
  <joint name="attacher_wrist_3_link-tool0_fixed_joint" type="fixed">
    <origin rpy="-1.57079632679 0 0" xyz="0 0.0922 0"/>
    <parent link="attacher_wrist_3_link"/>
    <child link="attacher_tool0"/>
  </joint>
  <joint name="attacher_world_joint" type="fixed">
    <parent link="world"/>
    <child link="attacher_base_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="1.5 0.0 0.0"/>
  </joint>
  <!-- Gazebo plugin for controlling joints -->
  <gazebo>
    <plugin filename="libgazebo_ros_control.so" name="ros_control"/>
  </gazebo>
</robot>

